{
  "schemaVersion": 1,
  "id": "axolotlclient-modmenu",
  "version": "${version}",
  "name": "AxolotlClient Modmenu",
  "description": "A simple way to configure your mods with Modmenu config screens!",
  "authors": [
    "moehreag"
  ],
  "contact": {
    "homepage": "https://moehreag.github.io/",
    "sources": "https://github.com/moehreag/AxolotlClient-modmenu"
  },
  "license": "LGPL-3.0-or-later",
  "icon": "assets/axolotlclient/icon.png",
  "environment": "*",
  "entrypoints": {
    "axolotlclient.module": [
      "io.github.axolotlclient.modmenu.AxolotlClientModmenu"
    ],
    "modmenu": [
      "io.github.axolotlclient.modmenu.AxolotlClientModmenuModmenuIntegration"
    ]
  },
  "depends": {
    "fabricloader": ">=0.14.0",
    "minecraft": ">=1.8.9",
    "axolotlclient": ">=2.2.0",
    "axolotlclientconfig": ">1.0.11"
